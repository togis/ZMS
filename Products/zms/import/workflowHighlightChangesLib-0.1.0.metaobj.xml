# --// getCustomBodyContent //--
from Products.zms import standard
request = container.REQUEST

subclass = ''
if standard.isPreviewRequest(REQUEST):
  if context.inObjStates( [ 'STATE_NEW'], REQUEST):
    subclass = subclass +' new'
  elif context.inObjStates( [ 'STATE_DELETED'], REQUEST):
    subclass = subclass +' deleted'
  elif context.inObjStates( [ 'STATE_MODIFIED'], REQUEST):
    subclass = subclass +' modified'
    
if len(subclass)>0:
  return """<div class="ZMSObject%s">%s</div>""" % (subclass,html)

return html

